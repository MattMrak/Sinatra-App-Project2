<h1>All Destinations</h1>

<ol>
  <% @destinations.each do |destination| %>
    <li>
      <strong>
        <a href="/destinations/<%= destination.id %>"><%= destination.location %></a>
      </strong> | <%= destination.date_visited %>
    </li>
  <% end %>
</ol>

<a href="/">Home |</a>
<a href="/destinations/new">New Destination |</a>
<a href="/logout">Logout</a>
